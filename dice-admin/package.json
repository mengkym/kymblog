{
  "name": "dice-admin",
  "version": "2.0.0",
  "description": "The admin of dice",
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build:prod": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "preview": "node build/index.js --preview",
    "lint": "eslint --ext .js,.vue src",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "dice",
    "vue",
    "admin",
    "element-ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bihell/Dice.git"
  },
  "bugs": {
    "url": "https://github.com/bihell/Dice/issues"
  },
  "dependencies": {
    "core-js": "3.6.5",
    "axios": "^0.19.1",
    "bulma-scss": "0.9.0-1",
    "clipboard": "2.0.6",
    "codemirror": "^5.50.2",
    "dayjs": "^1.8.19",
    "echarts": "^4.6.0",
    "element-ui": "2.13.2",
    "file-saver": "^2.0.2",
    "fuse.js": "6.3.0",
    "github-markdown-css": "4.0.0",
    "highlight.js": "10.1.1",
    "js-cookie": "^2.2.1",
    "jsonlint": "1.6.3",
    "jszip": "^3.2.2",
    "lodash": "^4.17.15",
    "mavon-editor": "^2.7.7",
    "mobile-detect": "^1.4.3",
    "normalize.css": "^8.0.1",
    "nprogress": "0.2.0",
    "path-to-regexp": "^3.2.0",
    "qs": "^6.9.1",
    "screenfull": "5.0.2",
    "sortablejs": "^1.10.2",
    "vue": "^2.6.11",
    "vue-count-to": "1.0.13",
    "vue-highlightjs": "^1.3.3",
    "vue-markdown": "^2.2.4",
    "vue-router": "3.3.4",
    "vue-splitpane": "1.0.6",
    "vuedraggable": "^2.23.2",
    "vuex": "^3.1.2",
    "script-loader": "0.7.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.4.6",
    "@vue/cli-plugin-eslint": "4.4.6",
    "@vue/cli-plugin-unit-jest": "4.4.6",
    "@vue/cli-service": "4.4.6",
    "@vue/test-utils": "1.0.3",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.1.0",
    "babel-core": "6.26.3",
    "chalk": "4.1.0",
    "chokidar": "^3.3.1",
    "connect": "^3.7.0",
    "eslint": "5.16.0",
    "eslint-plugin-vue": "6.2.2",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "runjs": "^4.3.2",
    "sass-loader": "8.0.2",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "serve-static": "^1.13.2",
    "svg-sprite-loader": "5.0.0",
    "svgo": "^1.3.2",
    "vue-ls": "^3.2.1",
    "vue-template-compiler": "2.6.11",
    "sass": "^1.26.2"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
